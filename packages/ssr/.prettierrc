{
	"bracketSpacing": true,
	"printWidth": 80,
	"proseWrap": "preserve",
	"semi": true,
	"trailingComma": "all",
	"tabWidth": 4,
	"useTabs": true,
	"arrowParens": "always",
	"requirePragma": false,
	"insertPragma": false,
	"endOfLine": "auto",
	"overrides": [
		{
			"files": ".*rc",
			"options": {
				"parser": "json"
			}
		}
	]
}
